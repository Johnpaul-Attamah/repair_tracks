{
  "name": "repair_tracks",
  "version": "1.0.0",
  "description": "Tracks office maintenance requests",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "dev": "SET NODE_ENV=development & npm run dev-server",
    "prod": "SET NODE_ENV=production & npm-run-all build prod-server",
    "test": "SET NODE_ENV=test & npm-run-all build stage-test",
    "prod-server": "node ./dist-src/server",
    "stage-test": "mocha ./dist-src/test/* --timeout=10000 --exit",
    "dev-server": "nodemon --exec babel-node ./src/server",
    "transpile": "babel ./src --out-dir dist-src",
    "clean": "rimraf dist-src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Johnpaul-Attamah/repair_tracks.git"
  },
  "keywords": [
    "repairs"
  ],
  "author": "Johnpaul Attamah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Johnpaul-Attamah/repair_tracks/issues"
  },
  "homepage": "https://github.com/Johnpaul-Attamah/repair_tracks#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gravatar": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.27",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
